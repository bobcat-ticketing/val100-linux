&spdif {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ais_spdif>;
	clocks = <&clks 197>, <&clks 0>,
		 <&clks 197>, <&clks 0>,
		 <&clks 0>,   <&clks 0>,
		 <&clks 0>,   <&clks 0>,
		 <&clks 0>;
	clock-names = "core",  "rxtx0",
		      "rxtx1", "rxtx2",
		      "rxtx3", "rxtx4",
		      "rxtx5", "rxtx6",
		      "rxtx7";
	status = "okay";
};

&ssi1 {
	fsl,mode = "i2s-slave";
	status = "okay";
};

&usbh1 {
	disable-over-current;
	vbus-supply = <&reg_usbh1_vbus>;
	status = "okay";
};

&usbotg {
	disable-over-current;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ais_usbotg_id>;
	vbus-supply = <&reg_usbotg_vbus>;
	dr_mode = "peripheral";
	status = "okay";
};

&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <
		&pinctrl_ais_usdhc2_aux
		&pinctrl_ais_usdhc2
	>;
	bus-width = <4>;
	/*cd-gpios = <&gpio1 4 0>;*/
	no-1-8-v;
	pm-ignore-notify;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	vmmc-supply = <&reg_3p3v>;
	status = "okay";
};

&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <
		&pinctrl_ais_usdhc3
	>;
	bus-width = <4>;
//	cd-gpios = <&gpio1 4 0>;
	no-1-8-v;
	pm-ignore-notify;
	keep-power-in-suspend;
//	cd-broken;
	enable-sdio-wakeup;
	max-frequency = <1000000>;
	vmmc-supply = <&reg_3p3v>;
	status = "okay";
};

&gpc {
	fsl,cpu_pupscr_sw2iso = <0xf>;
	fsl,cpu_pupscr_sw = <0xf>;
	fsl,cpu_pdnscr_iso2sw = <0x1>;
	fsl,cpu_pdnscr_iso = <0x1>;
};



&pwm1 {
        pinctrl-names = "default";
        status = "disabled";
};

&pwm2 {
        pinctrl-names = "default";
        status = "okay";
};

&pwm3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm3>;
        status = "okay";
	/*backlight*/
};

&pwm4 {
